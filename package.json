{
  "name": "markdown-editor",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "pnpm run format && tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview --host",
    "format": "npx prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\""
